# Courtside Main Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Build Configuration
VITE_API_URL=https://your-backend-domain.com
VITE_ADMIN_API_URL=https://your-admin-backend-domain.com

# Telegram Configuration
VITE_TELEGRAM_BOT_TOKEN=your-telegram-bot-token
VITE_TELEGRAM_BOT_USERNAME=your-telegram-bot-username

# Analytics
VITE_ANALYTICS_ID=your-analytics-id

# Environment
NODE_ENV=production
